{
  "rules": {
    "indent": [2, 2],
    "no-empty": [2],
    "no-console": [0],
    "comma-dangle": [2],
    "no-unused-vars": [0],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "curly": [2],
    "dot-notation": [2],
    "eqeqeq": [2],
    "radix": [2]
  },
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "es6": true,
    "experimentalObjectRestSpread": true
  }
}
